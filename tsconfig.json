{
    "compilerOptions": {
      "target": "ESNext",
      "lib": ["DOM", "ESNext"],
      "module": "ESNext",
      "moduleResolution": "Node",
      "jsx": "react-jsx",                           
      "esModuleInterop": true,                       
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "skipLibCheck": true,
      "resolveJsonModule": true,
      "noEmit": true,
      "baseUrl": "./src"
    },
    "include": ["src"]
  }
  